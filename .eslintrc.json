{
    "extends": "airbnb-base",
		"env": {
    	"browser": true,
    	"node": true
  	},
		"globals": {
			"document": false		  
		},
		"rules": {
        "indent": [2, "tab"],
        "no-tabs": 0,
		"no-unused-vars": ["error", { "vars": "all", "args": "after-used" }],
		"linebreak-style": 0
    }
}
